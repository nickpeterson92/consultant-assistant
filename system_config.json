{
  "database": {
    "path": "memory_store.db",
    "timeout": 30,
    "pool_size": 5,
    "auto_commit": true
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "external_logs_dir": "logs",
    "max_file_size": 10485760,
    "backup_count": 5,
    "buffer_size": 1000
  },
  "llm": {
    "model": "gpt-4o-mini",
    "temperature": 0.3,
    "top_p": 0.9,
    "max_tokens": 4000,
    "timeout": 120,
    "retry_attempts": 3,
    "retry_delay": 1.0,
    "cache_enabled": true,
    "cache_ttl": 3600,
    "azure_deployment": "gpt-4o-mini",
    "api_version": "2024-06-01",
    "pricing": {
      "gpt-4": {
        "input_per_1k": 0.03,
        "output_per_1k": 0.06
      },
      "gpt-4o": {
        "input_per_1k": 0.005,
        "output_per_1k": 0.015
      },
      "gpt-4o-mini": {
        "input_per_1k": 0.00015,
        "output_per_1k": 0.00060
      },
      "gpt-3.5-turbo": {
        "input_per_1k": 0.0005,
        "output_per_1k": 0.0015
      }
    }
  },
  "a2a": {
    "timeout": 60,
    "connect_timeout": 30,
    "sock_connect_timeout": 30,
    "sock_read_timeout": 60,
    "health_check_timeout": 10,
    "max_concurrent_calls": 10,
    "retry_attempts": 3,
    "retry_delay": 1.0,
    "circuit_breaker_threshold": 5,
    "circuit_breaker_timeout": 30,
    "connection_pool_size": 20,
    "connection_pool_ttl": 300,
    "connection_pool_max_idle": 300
  },
  "security": {
    "input_validation_enabled": true,
    "max_input_length": 50000
  },
  "conversation": {
    "summary_threshold": 5,
    "max_conversation_length": 50,
    "memory_extraction_enabled": true,
    "memory_extraction_delay": 0.5,
    "memory_update_turn_threshold": 3,
    "max_event_history": 50,
    "default_user_id": "user-1",
    "default_thread_id": "default",
    "memory_namespace_prefix": "memory",
    "summary_key": "conversation_summary",
    "memory_key": "SimpleMemory",
    "typing_effect_enabled": true,
    "typing_char_delay": 0.02,
    "typing_chunk_delay": 0.15,
    "typing_line_delay": 0.03,
    "typing_paragraph_delay": 0.15,
    "typing_first_line_char_limit": 100,
    "typing_instant_elements": true
  },
  "agents": {
    "salesforce-agent": {
      "endpoint": "http://localhost",
      "port": 8001,
      "enabled": true,
      "health_check_interval": 60,
      "max_memory_usage": 512,
      "timeout": 120
    },
    "jira-agent": {
      "endpoint": "http://localhost",
      "port": 8002,
      "enabled": true,
      "health_check_interval": 60,
      "max_memory_usage": 512,
      "timeout": 120
    }
  },
  "debug_mode": false,
  "environment": "development"
}